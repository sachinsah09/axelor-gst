<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

	<grid name="address-grid" title="Address-Details"
		model="com.axelor.gst.db.Address">
		<field name="type" />
		<field name="line1" />
		<field name="line2" />
		<field name="address" />
		<field name="city" />
		<field name="state" />
		<field name="country" />
		<field name="pincode" />
	</grid>

	<form name="address-form" title="Address-Form"
		model="com.axelor.gst.db.Address">
		<panel title="Fill all Details">
			<field name="type" />
			<field name="line1" />
			<field name="line2" />
			<field name="address" />
			<field name="city" />
			<field name="state" />
			<field name="country" />
			<field name="pincode" />
		</panel>
	</form>

	<grid name="city-grid" title="City-Details"
		model="com.axelor.gst.db.City">
		<field name="city" />
		<field name="state" />
		<field name="country" />
	</grid>
	<form name="city-form" title="City-Form"
		model="com.axelor.gst.db.City">
		<panel title="Fill all Details">
			<field name="city" />
			<field name="state" />
			<field name="country" />
		</panel>
	</form>

	<grid name="state-grid" title="State-Details"
		model="com.axelor.gst.db.State">
		<field name="name" />
		<field name="country" />
	</grid>
	<form name="state-form" title="State-Form"
		model="com.axelor.gst.db.State">
		<panel title="Fill all Details">
			<field name="name" />
			<field name="country" />
		</panel>
	</form>

	<grid name="country-grid" title="Country-Details"
		model="com.axelor.gst.db.Country">
		<field name="country" />
	</grid>
	<form name="country-form" title="Country-Form"
		model="com.axelor.gst.db.Country">
		<panel title="Fill all Details">
			<field name="country"/>
		</panel>
	</form>

</object-views>