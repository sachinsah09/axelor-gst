<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	<entity name="Product" sequential="true" cachable="true">
		<string name="name" required="true" />
		<string name="code" required="true" />
		<string name="hsbn" />
		<many-to-one name="category"
			ref="com.axelor.gst.db.Category" />
		<decimal name="salesPrice" />
		<decimal name="costPrice" />
		<binary name="image" />
		<decimal name="gstRate" />
	</entity>
	<entity name="ProductCategory" sequential="true" cachable="true">
		<string name="name" required="true" />
		<string name="code" required="true" />
		<decimal name="gstRate" />
	</entity>

</domain-models>

