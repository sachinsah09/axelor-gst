<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	<entity name="Address" sequential="true" cachable="true">
		<string name="type" required="true"
			selection="gst.gst.address.gst.address.type.select" />
		<string name="line1" required="true" />
		<string name="line2" />
		<many-to-one name="city" required="true"
			ref="com.axelor.gst.db.City" />
		<many-to-one name="state" ref="com.axelor.gst.db.State" />
		<many-to-one name="country" required="true"
			ref="com.axelor.gst.db.Country" />
		<string name="pinCode" />
	</entity>
	<entity name="City" sequential="true" cachable="true">
		<string name="city" required="true" />
		<many-to-one name="state" ref="com.axelor.gst.db.State" />
		<many-to-one name="country" required="true"
			ref="com.axelor.gst.db.Country" />
	</entity>
	<entity name="State" sequential="true" cachable="true">
		<string name="name" required="true" />
		<many-to-one name="country" required="true"
			ref="com.axelor.gst.db.Country" />
	</entity>
	<entity name="Country" sequential="true" cachable="true">
		<string name="country" required="true" />
	</entity>
</domain-models>

