<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	<entity name="BankAccount" sequential="true" cachable="true">
		<string name="bank" title="Bank Name" required="true" />
		<string name="accountNumber" title="Account Number" required="true" />
		<string name="ifsc" title="IFSC Code" required="true" />
		<string name="type" title="Acocunt Type" default="current" selection="gst.bankaccount.type.select"  />
		<string name="address" title="Bank Address"/>
	</entity>
</domain-models>

