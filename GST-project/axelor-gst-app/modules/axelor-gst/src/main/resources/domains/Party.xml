<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">


	<module name="gst" package="com.axelor.gst.db" />
	<entity name="Party" sequential="true" cachable="true">
		<string name="partySeq" readonly="true" default="" />
		<string name="name" required="true" />
		<string name="type" required="true"
			sequence="gst.gst.party.gst.party.type.select" default="company" />
		<boolean name="isCustomer" />
		<boolean name="isSupplier" />
		<string name="gstin" />
		<one-to-many name="contactList"
			ref="com.axelor.gst.db.Contact" />
		<one-to-many name="addressList"
			ref="com.axelor.gst.db.Address" />
	</entity>
</domain-models>

