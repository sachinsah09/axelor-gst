<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	<entity name="Party" sequential="true" cachable="true" repository="abstract">
		<string name="partySeq"  title="Party No." readonly="true"  default="" />
		<string name="name" required="true"  title="Party Name"/>
		<string name="type"  title="Party Type" required="true" default="company"
			sequence="gst.party.type.select" />
		<boolean name="isCustomer"  title="Party is a Customer"/>
		<boolean name="isSupplier"  title="Party is a Supplier"/>
		<string name="gstin"  title="GSTIN"/>
		<one-to-many name="contactList"  title="Contacts"
			ref="com.axelor.gst.db.Contact" />
		<one-to-many name="addressList"  title="Addresses"
			ref="com.axelor.gst.db.Address" />
	</entity>
</domain-models>

