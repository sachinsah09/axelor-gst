<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />
	<entity name="InvoiceLine" sequential="true" cachable="true">
		<many-to-one name="product"
			ref="com.axelor.gst.db.Product" />
		<many-to-one name="invoice"
			ref="com.axelor.gst.db.Invoice" />
		<string name="hsbn" />
		<string name="item" required="true" />
		<integer name="qty" />
		<decimal name="price" />
		<decimal name="netAmount" />
		<decimal name="gstRate" />
		<decimal name="igst" />
		<decimal name="sgst" />
		<decimal name="cgst" />
		<decimal name="grossAmount" />
	</entity>
</domain-models>

